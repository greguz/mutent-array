{
  "name": "mutent-array",
  "version": "0.2.0",
  "description": "Simple array adapter for mutent",
  "type": "module",
  "main": "./mutent-array.cjs",
  "exports": {
    "import": "./mutent-array.mjs",
    "require": "./mutent-array.cjs"
  },
  "types": "./mutent-array.d.ts",
  "files": [
    "mutent-array.cjs",
    "mutent-array.mjs",
    "mutent-array.d.ts"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "standard **/*.mjs",
    "test": "c8 -r html -r lcov -r text  -x **/*.spec.mjs ava",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "keywords": [
    "memory",
    "mutent",
    "adapter",
    "array"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "peerDependencies": {
    "mutent": "5.0.0-9"
  },
  "devDependencies": {
    "ava": "4.0.0-alpha.2",
    "c8": "^7.10.0",
    "mutent": "5.0.0-9",
    "rollup": "^2.58.0",
    "standard": "^16.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mutent-array.git"
  }
}
